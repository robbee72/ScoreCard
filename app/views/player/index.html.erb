<h4>Listing of all Players</h4>
 <p>
   <%= link_to "Create new player", new_player_path %>
 </p>
 <table>
   <tr>
     <th>Name</th>
     <th>Ranking</th>
   </tr>
   <% @players.each do |player| %>
   <tr>
     <td><%= player.name %></td>
     <td><%= player.ranking %></td>
     <td><%= link_to 'Edit', edit_player_path(player) %></td>
     <td><%= link_to 'Show', player_path(player) %></td>
   </tr>
   <% end %>
 </table>
